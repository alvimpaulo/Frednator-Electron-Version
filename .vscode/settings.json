{
  "editor.formatOnSave": true,
  "files.associations": {
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "array": "cpp",
    "*.tcc": "cpp",
    "cstdint": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "ostream": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "type_traits": "cpp",
    "tuple": "cpp",
    "typeinfo": "cpp",
    "utility": "cpp",
    "complex": "cpp",
    "deque": "cpp",
    "new": "cpp",
    "optional": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "algorithm": "cpp",
    "chrono": "cpp",
    "ctime": "cpp",
    "ratio": "cpp",
    "csignal": "cpp",
    "atomic": "cpp",
    "strstream": "cpp",
    "bitset": "cpp",
    "list": "cpp",
    "slist": "cpp",
    "iomanip": "cpp",
    "thread": "cpp",
    "typeindex": "cpp",
    "memory": "cpp",
    "string": "cpp",
    "mutex": "cpp"
  },
  "lldb.executable": "lldb-7",
  "files.watcherExclude": {
    "**/build/**": true
  },
  "C_Cpp.intelliSenseCachePath": "${workspaceFolder}/.cache",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "C_Cpp.loggingLevel": "Warning",
  "search.usePCRE2": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "backgroundColor": "transparent"
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "**",
      "color": "#98C379",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "$",
      "color": "#FFFF00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true
}
